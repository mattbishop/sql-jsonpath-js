    "iterare": "^1.2.1",
    "luxon": "^3.1.0"
  },
  "devDependencies": {
    "@types/chai": "4.3.4",
    "@types/luxon": "3.1.0",
    "@types/mocha": "10.0.0",
    "@types/node": "18.11.9",
    "chai": "4.3.7",
    "mocha": "10.1.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.3"
